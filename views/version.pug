if row.status=='ok'
    td
        span(class='fa fa-arrow-right')= ' '
            a= row.version
else if row.lastGodBuildVersion
    td
        -let formatedDate = prettyDate(row.lastGodBuildTime)
        span(class="fa fa-calendar", title=formatedDate)= ' '
            a= row.lastGodBuildVersion
else
    td(class="fa fa-question-circle build-missing")
